<?xml version="1.0" encoding="UTF-8"?>
<BWSharedResource>
    <name>LocalJMSConnection</name>
    <resourceType>ae.shared.JMSConnectionKey</resourceType>
    <config>
        <NamingEnvironment>
            <UseJNDI>true</UseJNDI>
            <ProviderURL>tcp://localhost:7222</ProviderURL>
            <NamingURL>%%EnterpriseResources/Resources/LocalJMSConnection/JMSURL%%</NamingURL>
            <NamingInitialContextFactory>%%EnterpriseResources/Resources/LocalJMSConnection/ContextFactory%%</NamingInitialContextFactory>
            <TopicFactoryName>%%EnterpriseResources/Resources/LocalJMSConnection/TopicConnectionFactory%%</TopicFactoryName>
            <QueueFactoryName>%%EnterpriseResources/Resources/LocalJMSConnection/QueueConnectionFactory%%</QueueFactoryName>
            <NamingPrincipal>%%EnterpriseResources/Resources/LocalJMSConnection/UserName%%</NamingPrincipal>
            <NamingCredential>%%EnterpriseResources/Resources/LocalJMSConnection/Password%%</NamingCredential>
        </NamingEnvironment>
        <ConnectionAttributes>
            <username>%%EnterpriseResources/Resources/LocalJMSConnection/UserName%%</username>
            <password>%%EnterpriseResources/Resources/LocalJMSConnection/Password%%</password>
            <clientID/>
            <autoGenClientID>true</autoGenClientID>
        </ConnectionAttributes>
        <UseXACF>false</UseXACF>
    </config>
</BWSharedResource>